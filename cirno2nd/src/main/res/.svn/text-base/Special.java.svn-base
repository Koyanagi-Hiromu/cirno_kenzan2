package main.res;

public class Special {
	public final String NAME;
	public final int LENGTH;
	public final boolean DIRECTION_ONLY_2;

	public Special(String name, int length) {
		NAME = name;
		LENGTH = length;
		DIRECTION_ONLY_2 = false;
	}

	public Special(String name, int length, boolean direction1) {
		NAME = name;
		LENGTH = length;
		DIRECTION_ONLY_2 = direction1;
	}
}