	private static final long serialVersionUID = 1L;

	public final static Checker_ClassName<Checker> NULL = new Checker_ClassName<Checker>();

	protected static ArrayList<Checker_ClassName_Enum> initList(
			Checker_ClassName_Enum[] values) {
		ArrayList<Checker_ClassName_Enum> list = new ArrayList<Checker_ClassName_Enum>();
		HashMap<Integer, Boolean> hash = new HashMap<Integer, Boolean>(
				values.length);
		for (int i = 0; i < values.length; i++)
			hash.put(i, false);
		for (int i = 0; i < values.length; i++) {
			int index = new R().nextInt(values.length - i);
			int j = 0;
			while (index >= j) {
				if (hash.get(j++))
					index++;
			}
			hash.put(index, true);
			list.add(values[index]);
		}
		return list;
	}

	public Checker get(Base_Artifact a) {
		if (isEmpty())
			return new Checker();
		for (Checker c : this) {
			if (c.clazz == null) {
				return c.set(a);
			}
			if (c.clazz == a.getClass())
				return c;
		}
		@SuppressWarnings("unchecked")
		E e = (E) new Checker(new Checker_ClassName_Enum() {
			@Override
			public String toString() {
				return "(用意された未識別の名前を使いきりました)";
			}
		});
		add(e);
		return e.set(a);
	}

