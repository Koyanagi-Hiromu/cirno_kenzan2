package dangeon.model.object.artifact.item.spellcard.set.effect;

import dangeon.model.object.artifact.item.spellcard.set.BSE;

public class SetEffectStatus {
	public static int getDEF() {
		if (BSE.STR.isCheck()) {
			return 20;
		}
		return 0;
	}

	public static int getHP() {
		if (BSE.HP.isCheck()) {
			return 100;
		}
		return 0;
	}

	public static int getSTR() {
		if (BSE.STR.isCheck()) {
			return 20;
		}
		return 0;
	}
}
